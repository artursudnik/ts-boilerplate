{
  "name": "ts-boilerplate",
  "version": "0.0.1",
  "description": "",
  "keywords": [],
  "license": "ISC",
  "author": "",
  "scripts": {
    "build": "pnpm check-deps && rimraf dist && tsc --project tsconfig.build.json",
    "check-deps": "check-dependencies",
    "check-types": "pnpm check-deps && tsc --noEmit",
    "generate-barrels": "barrelsby --config barrelsby.config.json",
    "lint": "pnpm check-deps && eslint_d \"**/*.ts\"",
    "prepare": "husky",
    "prettier:check": "pnpm check-deps && prettier -c .",
    "prettier:write": "pnpm check-deps && prettier -w .",
    "start:dev": "pnpm check-deps && pnpm tsx watch --clear-screen=false src/index.ts",
    "test": "pnpm check-deps && jest"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "20.2.0",
    "@commitlint/config-conventional": "20.2.0",
    "@eslint/js": "9.39.2",
    "@types/jest": "30.0.0",
    "@types/node": "24.10.4",
    "barrelsby": "2.8.1",
    "check-dependencies": "2.0.0",
    "eslint": "9.39.2",
    "eslint_d": "14.3.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.4",
    "globals": "16.5.0",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "lint-staged": "16.2.7",
    "organize-imports-cli": "0.10.0",
    "prettier": "3.7.4",
    "prettier-plugin-organize-imports": "4.3.0",
    "prettier-plugin-packagejson": "2.5.20",
    "rimraf": "6.1.2",
    "ts-jest": "29.4.6",
    "ts-node": "10.9.2",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.50.1"
  },
  "packageManager": "pnpm@10.26.2+sha512.0e308ff2005fc7410366f154f625f6631ab2b16b1d2e70238444dd6ae9d630a8482d92a451144debc492416896ed16f7b114a86ec68b8404b2443869e68ffda6",
  "engines": {
    "node": "24",
    "npm": "use yarn",
    "yarn": "1"
  },
  "engineStrict": true
}
