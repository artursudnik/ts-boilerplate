{
  "name": "ts-boilerplate",
  "version": "0.0.1",
  "description": "",
  "keywords": [],
  "license": "ISC",
  "author": "",
  "scripts": {
    "build": "pnpm check-deps && rimraf dist && tsc --project tsconfig.build.json",
    "check-deps": "check-dependencies",
    "check-types": "pnpm check-deps && tsc --noEmit",
    "generate-barrels": "barrelsby --delete --singleQuotes --location all --directory ./src",
    "lint": "pnpm check-deps && eslint_d \"**/*.ts\"",
    "prepare": "husky",
    "prettier:check": "pnpm check-deps && prettier -c .",
    "prettier:write": "pnpm check-deps && prettier -w .",
    "test": "pnpm check-deps && jest"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.14",
    "@typescript-eslint/eslint-plugin": "8.28.0",
    "@typescript-eslint/parser": "8.28.0",
    "barrelsby": "2.8.1",
    "check-dependencies": "2.0.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-prettier": "5.2.6",
    "eslint_d": "14.3.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "lint-staged": "15.5.0",
    "organize-imports-cli": "0.10.0",
    "prettier": "3.5.3",
    "prettier-plugin-organize-imports": "4.1.0",
    "prettier-plugin-packagejson": "2.5.10",
    "rimraf": "6.0.1",
    "ts-jest": "29.3.1",
    "ts-node": "10.9.2",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@10.7.1+sha512.2d92c86b7928dc8284f53494fb4201f983da65f0fb4f0d40baafa5cf628fa31dae3e5968f12466f17df7e97310e30f343a648baea1b9b350685dafafffdf5808",
  "engines": {
    "node": "22",
    "npm": "use yarn",
    "yarn": "1"
  },
  "engineStrict": true
}
